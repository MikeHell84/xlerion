        "type": "email",
        "required": true,
        "placeholder": "tu@email.com"
      },
      {
        "name": "telefono",
        "label": "Teléfono / WhatsApp",
        "type": "tel",
        "required": false,
        "placeholder": "+57 3XX XXXXXX"
      },
      {
        "name": "ubicacion",
        "label": "Tu ubicación en la vereda",
        "type": "text",
        "required": false,
        "placeholder": "Barrio, sector o vereda"
      },
      {
        "name": "categoria",
        "label": "Tipo de contenido",
        "type": "select",
        "required": true,
        "options": [
          { "value": "noticia", "label": "Noticia" },
          { "value": "reportaje", "label": "Reportaje" },
          { "value": "musica", "label": "Música" },
          { "value": "testimonio", "label": "Testimonio" },
          { "value": "entrevista", "label": "Entrevista" },
          { "value": "otro", "label": "Otro" }
        ]
      },
      {
        "name": "titulo",
        "label": "Título de tu contenido",
        "type": "text",
        "required": true,
        "placeholder": "¿De qué trata tu historia?"
      },
      {
        "name": "descripcion",
        "label": "Descripción breve",
        "type": "textarea",
        "required": true,
        "placeholder": "Cuéntanos en pocas palabras de qué se trata",
        "rows": 4
      },
      {
        "name": "archivo_opcion",
        "label": "¿Cómo deseas enviar tu contenido?",
        "type": "radio",
        "required": true,
        "options": [
          { "value": "archivo", "label": "Subir archivo de audio (MP3, WAV)" },
          { "value": "whatsapp", "label": "Enviar por WhatsApp" },
          { "value": "enlace", "label": "Compartir enlace externo" }
        ]
      },
      {
        "name": "archivo_audio",
        "label": "Tu archivo de audio",
        "type": "file",
        "required": false,
        "accept": "audio/*",
        "max_size": 52428800,
        "conditional_field": "archivo_opcion",
        "conditional_value": "archivo"
      },
      {
        "name": "enlace_url",
        "label": "URL de tu contenido",
        "type": "url",
        "required": false,
        "placeholder": "https://...",
        "conditional_field": "archivo_opcion",
        "conditional_value": "enlace"
      },
      {
        "name": "consentimiento",
        "label": "Autorizo a Estación Comunitaria Nocaima a reproducir y editar mi contenido",
        "type": "checkbox",
        "required": true
      },
      {
        "name": "privacidad",
        "label": "He leído y acepto la política de privacidad",
        "type": "checkbox",
        "required": true
      }
    ],
    "form_endpoint": "/api/correspondents/submit",
    "form_method": "POST",
    "success_message": "¡Gracias por tu contribución! Revisaremos tu contenido y nos pondremos en contacto en los próximos 2-3 días hábiles.",
    "whatsapp_link": "https://wa.me/573001234567?text=Quiero%20ser%20corresponsal%20de%20Estaci%C3%B3n%20Nocaima",
